<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="4" id="com.crewuntold" version="1.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Crew Untold</name>
    <description>The untold stories of air stewards everywhere</description>
    <author email="hi@designplox.co" href="https://designplox.com">Ali Zahid</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="android-minSdkVersion" value="19" />
        <preference name="CodePushDeploymentKey" value="awmX6c2n5i6q-shQzM_4IXdq-hGLVy5x7ifwM" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="CodePushDeploymentKey" value="rtvPn9CTkEvT1UjVr2GO4uO8AcIlVy5x7ifwM" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarStyle" value="default" />
    <preference name="KeyboardShrinksView" value="true" />
    <hook src="scripts/artwork.js" type="before_prepare" />
    <engine name="android" spec="~6.2.2" />
    <engine name="ios" spec="~4.3.1" />
    <plugin name="com.verso.cordova.clipboard" spec="https://github.com/VersoSolutions/CordovaClipboard" />
    <plugin name="cordova-plugin-actionsheet" spec="~2.3.3" />
    <plugin name="cordova-plugin-apprate" spec="~1.2" />
    <plugin name="cordova-plugin-code-push" spec="~1.9.3-beta" />
    <plugin name="cordova-plugin-device" spec="~1.1.5" />
    <plugin name="cordova-plugin-dialogs" spec="~1.3.2" />
    <plugin name="cordova-plugin-fcm" spec="~2.1.1" />
    <plugin name="cordova-plugin-keyboard" spec="~1.1.5" />
    <plugin name="cordova-plugin-statusbar" spec="~2.2.2" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-x-socialsharing" spec="~5.1.8" />
</widget>
